{
  "name": "ts-config",
  "version": "0.0.0-development",
  "description": "An opinionated TSLint configuration preset. Works best together with strict TypeScript settings and Prettier.",
  "license": "MIT",
  "author": "Clemens Akens <clebert@me.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/clebert/ts-config.git"
  },
  "bugs": {
    "url": "https://github.com/clebert/ts-config/issues"
  },
  "files": [
    "bin/",
    "dist/",
    "tsconfig.json"
  ],
  "bin": {
    "ts-config": "bin/run"
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "commitmsg": "commitlint --edit",
    "precommit": "lint-staged"
  },
  "oclif": {
    "commands": "dist/commands",
    "bin": "ts-config",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "dependencies": {
    "@oclif/command": "^1.4.36",
    "@oclif/errors": "^1.1.2",
    "@oclif/plugin-help": "^2.0.5",
    "common-tags": "^1.8.0",
    "json-diff": "^0.5.2",
    "prettier": "^1.13.7",
    "semver": "^5.5.0"
  },
  "peerDependencies": {
    "tslint": "^5.10.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-angular": "^7.0.1",
    "@commitlint/travis-cli": "^7.0.0",
    "@oclif/config": "^1.6.33",
    "@types/common-tags": "^1.4.0",
    "@types/node": "^10.5.2",
    "@types/prettier": "^1.13.2",
    "@types/semver": "^5.5.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "semantic-release": "^15.7.2",
    "travis-deploy-once": "^5.0.1",
    "tslint": "^5.11.0",
    "typescript": "~2.9.2"
  }
}
